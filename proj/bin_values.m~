function [hist] = bin_values(features, radius)
%BIN_VALUES Summary of this function goes here
%   Detailed explanation goes here

hist = zeros(16, 1);

for n = 1:length(features)

    binno = 0;
    
    si = 0; %can change for bins
    s2 = radius;
    
    f1 = features(n,1);
    f2 = features(n,2);
    f3 = features(n,3);
    f4 = features(n,4);
    
    
    
    
    
end

